    "url-loader": "^4.1.1",
    "vite": "^2.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.tsx": "eslint --fix --ext .tsx",
    "src/**/*.ts": "eslint --fix --ext .ts",
    "src/**/*.jsx": "eslint --fix --ext .jsx",
    "src/**/*.js": "eslint --fix --ext .js",
    "src/**/*.vue": "eslint --fix --ext .vue"
  }
}
